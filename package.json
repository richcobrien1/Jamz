{
  "name": "jamz",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "frontend/client",
    "backend"
  ],
  "scripts": {
    "start:frontend": "cd frontend/client && npm start",
    "start:backend": "cd backend && npm start",
    "dev": "concurrently \"npm run start:backend\" \"npm run start:frontend\"",
    "build": "cd frontend/client && npm run build",
    "deploy": "vercel --prod"
  }
}
  