{
  "version": 2,
  "builds": [
        {
      "src": "frontend/client/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "frontend/client"
      }
    },
    {
      "src": "backend/src/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "backend/src/index.js"
    }
  ]
}
